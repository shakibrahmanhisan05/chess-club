# Frontend Environment Variables
# Copy this file to .env for local development
# For Vercel deployment, set these in the Vercel dashboard under Settings > Environment Variables

# Backend API URL (required)
# For local development: http://localhost:8000
# For production: https://your-backend.onrender.com
REACT_APP_BACKEND_URL=https://your-backend-url.onrender.com

# WebSocket port for development server (optional)
WDS_SOCKET_PORT=443

# Enable health check endpoint (optional)
ENABLE_HEALTH_CHECK=false
